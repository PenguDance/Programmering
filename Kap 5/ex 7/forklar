function setup() {
    createCanvas(400, 400);
    let wordcloud =["Javascript","Python","C++","C","C#","Julia","Java","Go","R","Basic"]/*Laver en liste med 
    de forskellige kodesprog*/
    for(let i = 0;i<wordcloud.length;i++)//Laver en for-løkke der kører indtil at i er lig med længden af listen
      {
        text(wordcloud[i],random(400),random(400))//Skriver navnet på kodesprog nummer i et tilfældigt sted på skærmen
        i=i+1//gør i større igen, så den sprænger hver anden over.
      }
  }